/* components/MusicListTemp/MusicListTemp.wxss */
.musiclist-temp{
  width: 100%;
  box-sizing: border-box;
  border-top-left-radius: 20px;
  background-color: white;
  padding: 4%;
  margin: 0 auto;
}
.ml-flex{
  display: flex;
  align-items: center;
  justify-content: space-between;
  margin-bottom: 4%;
}
.ml-text{
  line-height: 40px;
  font-size: 18px;
  color: black;
}
.icon-big{
  width: 100%;
  height: 40px;
  border-radius: 20px;
  background-color: #f4ea2a;
  display: flex;
  align-items: center;
}
.icon-left{
  border-radius: 15px;
  background-color: black;
  width: 30px;
  height: 30px;
  display: flex;
  align-items: center;
  justify-content: center;
  margin-left: 5px;
}
.icon-right{
  margin-left: 10px;
  font-size: 15px;
  margin-right: 10px;
}
.icon-imgset{
  display: block;
  width: 60%;
}

.item-set{
  margin-bottom: 15px;
}
