/* components/mvItem/mvItem.wxss */
.mvItemClass{
  width: 100%;
  box-sizing: border-box;
  display: flex;
}
.mvItemClass view{
  box-sizing: border-box;
}
.mvImage{
  width: 40%;
}
.textConner{
  width: 60%;
  padding-left: 5px;
  display: flex;
  flex-direction:column-reverse;
}
.markName{
  font-size: 12px;
  color: #aaaaaa;

  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.artistName{
  font-size: 16px;
  line-height: 20px;

  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}